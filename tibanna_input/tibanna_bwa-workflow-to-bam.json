{
  "args": {
    "app_name": "bwa-workflow-to-bam",
    "app_version": "v2",
    "cwl_directory_url": "https://raw.githubusercontent.com/dbmi-bgm/cgap-pipeline/master/",
    "cwl_main_filename": "bwa-workflow-to-bam.cwl",
    "cwl_child_filenames": ["gunzip.cwl", "bwa-mem_to_stdout.cwl", "sam-to-bam_from_stdout.cwl"],
    "cwl_version": "v1",
    "input_files": {
      "reads_gzipped": {
        "bucket_name": "maestro-resources",
        "object_key": "PSC-01-003_down3x_R1.fastq.gz"
      },
      "mates_gzipped": {
        "bucket_name": "maestro-resources",
        "object_key": "PSC-01-003_down3x_R2.fastq.gz"
      },
      "reference": {
        "bucket_name": "maestro-resources",
        "object_key": "GRCh38_full_analysis_set_plus_decoy_hla.fa.bwt"
      }
    },
    "secondary_files": {
      "reference": {
        "bucket_name": "maestro-resources",
        "object_key": ["GRCh38_full_analysis_set_plus_decoy_hla.fa.ann", "GRCh38_full_analysis_set_plus_decoy_hla.fa.amb", "GRCh38_full_analysis_set_plus_decoy_hla.fa.pac", "GRCh38_full_analysis_set_plus_decoy_hla.fa.sa"]
      }
    },
    "input_parameters": {
      "threads": 72
    },
    "output_S3_bucket": "soos-4dn-bucket",
    "output_target": {
      "output": "prova_workflow/prova_bwa_workflow_down3x.bam"
    },
    "secondary_output_target": {}
  },
  "config": {
    "instance_type": "c5n.18xlarge",
    "ebs_size": "6.25x",
    "EBS_optimized": true,
    "spot_instance": true,
    "log_bucket": "soos-4dn-bucket",
    "run_name": "run_bwa-workflow-to-bam",
    "behavior_on_capacity_limit": "wait_and_retry"
  }
}
