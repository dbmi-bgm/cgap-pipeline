{
  "args": {
    "app_name": "sort-bam",
    "app_version": "v2",
    "cwl_directory_url": "https://raw.githubusercontent.com/dbmi-bgm/cgap-pipeline/master/",
    "cwl_main_filename": "sort-bam.cwl",
    "cwl_child_filenames": [],
    "cwl_version": "v1",
    "input_files": {
      "input": {
        "bucket_name": "",
        "object_key": ""
      }
    },
    "secondary_files": {},
    "input_parameters": {
      "max_memory": "3G",
      "num_threads": 8
    },
    "output_S3_bucket": "soos-4dn-bucket",
    "output_target": {
      "output": "prova_workflow/prova_sortedbam_down3x.bam"
    },
    "secondary_output_target": {}
  },
  "config": {
    "instance_type": "c5n.18xlarge",
    "ebs_size": "6.25x",
    "EBS_optimized": true,
    "spot_instance": true,
    "log_bucket": "soos-4dn-bucket",
    "run_name": "run_bwa-workflow-to-bam",
    "behavior_on_capacity_limit": "wait_and_retry"
  }
}
